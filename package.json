{
  "name": "mind-garden",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:seq": "NODE_ENV=test ENV=test jest --runInBand --testPathPattern='/src/' --bail=false --passWithNoTests",
